import java.util.*;
class A{
    void overRide(int i){
        System.out.println("method 1:value of i:"+i);
    }
}
class B extends A{
    void overRide(int i){
        super.overRide(5);
        i++;
        System.out.println("Method 2: value of i:"+i);
    }
    public static void main(String []args){
        B b=new B();
        b.overRide(2);
    }
}
